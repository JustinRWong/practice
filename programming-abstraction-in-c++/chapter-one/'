#include <iostream>
using namespace std;

#define MAX 10000

double square(double x)
{
    return x *x;
}
double compute_quarter_circle(double radius)
{
    double sum = 0;
    for(int i = 1; i <= MAX; i++)
    {
        int height= sqrt(square(r)- square(i*r/MAX));
        sum ++ 
    }
}
